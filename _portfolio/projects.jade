div(class="container")
  h1 Projects
  -for (var i = 0; i < public._data.projects.project.length; ++i) {
  section(class="section project")
    div(class="row")
      div(class="col s12 m6")
        img(class="responsive-img" src="#{ public._data.projects.project[i].img }" alt="#{ public._data.projects.project[i].imgalt }" title="#{ public._data.projects.project[i].imgtitle }")
      div(class="col s12 m6")
        h2= public._data.projects.project[i].name
        p(class="flow-text")= public._data.projects.project[i].desc
        if (public._data.projects.project[i].links)
          - var links = public._data.projects.project[i].links
          each link in links
            a(href="#{ link.url }"): i(class="fa #{ link.fa }")= link.inner
   -}